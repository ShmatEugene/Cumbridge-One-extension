{
    "name": "Cumbridge One",
    "description": "Cambridge One Solver",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "tabs", "webRequest", "declarativeNetRequest"],
    "host_permissions": ["https://www.cambridgeone.org/*", "*://content.cambridgeone.org/*"],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["fix.css"],
        "matches": ["https://www.cambridgeone.org/*"]
      }
    ],
    "content_scripts": [
        {
            "matches": ["https://www.cambridgeone.org/*"],
            "js": ["content.js"],
            "css": ["fix.css"]
        }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Cumbridge One",
      "default_icon": {
        "16": "/images/cumbridge16.png",
        "32": "/images/cumbridge32.png",
        "48": "/images/cumbridge48.png",
        "128": "/images/cumbridge128.png"
      }
    },
    "icons": {
      "16": "/images/cumbridge16.png",
      "32": "/images/cumbridge32.png",
      "48": "/images/cumbridge48.png",
      "128": "/images/cumbridge128.png"
    }
  }